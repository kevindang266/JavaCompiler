%namespace GPLexTutorial

%union
{
    public int num;
    public string name;
}

%token <num> Number
%token <name> Identifier 
%token If Else Int Boolean BooleanLiteral NullLiteral CharacterLiteral

%left '='
%nonassoc '<'
%left '+'

